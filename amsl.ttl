    owl:oneOf ( amsl:TeachingHospitals  amsl:Staff amsl:Students  amsl:UniMember amsl:WalkIns ) .
    
amsl:Staff a amsl:UserGroup ;
    rdfs:label "Mitarbeiter"@de,
        "Staff"@en .
        
amsl:Students a amsl:UserGroup ;
    rdfs:label "Studierende"@de,
        "Students"@en .
        
