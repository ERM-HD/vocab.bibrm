amsl:ShibbolethImplStatus a owl:Class ;
    owl:oneOf ( amsl:Inquired amsl:Prospected amsl:InProcess amsl:Implemented amsl:Impossible   ) .

amsl:MetadataSourceImplStatus a owl:Class ;
    owl:oneOf   (amsl:Wish amsl:Inquired  amsl:InProcess amsl:Trial amsl:Active  amsl:Impossible ) .

amsl:ERStatus a owl:Class ;
    owl:oneOf ( amsl:Active amsl:CancellationNominee amsl:Cancelled  amsl:Trial amsl:Wish  ) .  


amsl:Active a amsl:ERStatus , amsl:MetadataSourceImplStatus ;
    rdfs:label "Aktiv"@de,
        "Active"@en .
        
amsl:CancellationNominee a amsl:ERStatus ;
    rdfs:label "Abbestellkandidat"@de,
        "Nominee for Cancellation"@en .
        
amsl:Cancelled a amsl:ERStatus ;
    rdfs:label "Abbestellt"@de,
        "Cancelled"@en .    

amsl:Implemented a amsl:ShibbolethImplStatus ;
    rdfs:label "Implementiert"@de,
        "Implemented"@en .

amsl:Impossible a amsl:ShibbolethImplStatus , amsl:MetadataSourceImplStatus ;
    rdfs:label "Nicht möglich"@de,
        "Not possible"@en .

amsl:InProcess a amsl:ShibbolethImplStatus , amsl:MetadataSourceImplStatus ;
    rdfs:label "In Bearbeitung (techn.)"@de,
        "In Process (techn.)"@en ;
    rdfs:comment "Wird momentan technisch bearbeitet."@de, "Is being technically processed"@en .

amsl:Inquired a amsl:ShibbolethImplStatus , amsl:MetadataSourceImplStatus ;
    rdfs:label "Angefragt / In Verhandlung"@de,
        "Inquired / In Negotiation"@en .
        
amsl:Prospected a amsl:ShibbolethImplStatus  ;
    rdfs:label "In Aussicht"@de,
        "Prospected"@en .

        
amsl:Trial a amsl:ERStatus , amsl:MetadataSourceImplStatus ;
    rdfs:label "Test"@de,
        "Trial"@en .

amsl:Wish a amsl:ERStatus , amsl:MetadataSourceImplStatus ;
    rdfs:label "Wunsch"@de,
        "Wish"@en .    






###### Article Index

amsl:Source a owl:Class ;
rdfs:label "Quelle"@de, "Source"@en ;
rdfs:comment "Eine Quelle, die nicht näher bestimmte Daten anbietet. Detailliertere Beschreibungen sind über Unterklassen möglich."@de, 
"A source providing not further specified data. Subclasses are available or a more detailed description."@en .

amsl:MetadataSource a owl:Class;
rdfs:subClassOf amsl:Source ;
rdfs:label "Metadatenquelle"@de, "Metadata Source"@en ;
rdfs:comment "Eine Quelle, die (bibliographische) Metadaten anbietet."@de,
 "A source providing (bibliographic) metadata"@en .


amsl:Collection a owl:Class ;
rdfs:label "Kollektion"@de, "Collection"@en ;
rdfs:comment "Eine Kollektion oder Sammlung, die nicht näher bestimmte zusammengehörige Daten umfasst. Detailliertere Beschreibungen sind über Unterklassen möglich."@de, 
"A collection containing not further specified data. Subclasses are available or a more detailed description."@en .


amsl:MetadataCollection a owl:Class ;
rdfs:subClassOf amsl:Collection ;
rdfs:label "Metadatenkollektion"@de, "Metadata Collection"@en;
rdfs:comment "Eine Kollektion von Metadaten, die bspw. in einen Bibliothekskatalog eingespielt wird."@de,
 "A collection of metadata that is to be integrated in e.g. a library catalog."@en .


amsl:metadataSourceImplStatus a owl:ObjectProperty ;
rdfs:label "Status der Metadaten-Implementierung"@de , "Metatadata Implementation status"@en ;
rdfs:comment "Verknüpft Informationen über den Status der Implementierung der Metadatenquelle im Artikelindex."@de , "Links information on the implementation status of the metadata source in the article index."@en ;
rdfs:domain amsl:MetadataSource ;
rdfs:range amsl:MetadataSourceImplStatus .

amsl:metadataUsedByLibrary a owl:ObjectProperty;
rdfs:label "Metadaten nutzende Bibliothek"@de, "metadata using library"@en ;
rdfs:comment "Verknüpft die Bibliothek, die die Metadaten einer Kollektion nutzt."@de, "Links the library using a collection's metadata."@en ;
rdfs:domain amsl:MetadataCollection ;
rdfs:range foaf:Organization .

amsl:sourceID a owl:DatatypeProperty ;
rdfs:label "Source ID"@de, "Source ID"@en;
rdfs:comment "Verknüpft die Source ID einer Metadatenquelle"@de, "Links the source ID of a metadata source"@en ;
rdfs:domain amsl:Source .

amsl:collectionID a owl:DatatypeProperty ;
rdfs:label "Collection ID"@de, "Collection ID"@en;
rdfs:comment "Verknüpft die Collection ID einer Metadatenkollektion"@de, "Links the collection ID of a metadata collection"@en ;
rdfs:domain amsl:Collection .

amsl:includedInMetadataSource a owl:ObjectProperty ;
rdfs:label "enthalten in Metadatenquelle"@de, "included in metadata source"@en ;
rdfs:comment "Verknüpft die die Kollektion enthaltende Metadatenquelle."@de , "Links the metadata source including the collection."@en ;
rdfs:domain amsl:MetadataCollection ;
rdfs:range amsl:MetadataSource .

amsl:indexingLevel a owl:DatatypeProperty ;
rdfs:label "Indexierungsgrad"@de, "indexing level"@en;
rdfs:comment "Verknüpft Angaben zum gebotenen Indexierungsgrad"@de, "Links information on the indexing level offered."@en ;
rdfs:domain amsl:MetadataSource .

amsl:updateRhythm a owl:DatatypeProperty ;
rdfs:label  "Update-Rhythmus"@de , "update rhythm"@en ;
rdfs:comment "Verknüpft Angaben zum Aktualiserungsrhythmus der angebotenen Metadaten"@de , "Links information on the update rhythm of the offered metadata."@en ;
rdfs:domain amsl:Source .

amsl:nextUpdateDate a owl:DatatypeProperty ;
rdfs:label  "Datum des nächsten Updates"@de , "date of next update"@en ;
rdfs:comment "Verknüpft das Datum der nächsten Aktualisierung der angebotenen Metadaten"@de , "Links the date of the next update of the offered metadata."@en ;
rdfs:domain amsl:Source ;
rdfs:range xsd:date .

amsl:linkToHoldingsFile a owl:DatatypeProperty ;
rdfs:label "Link zum Holdings-File"@de, "Link to holdings file"@en ;
rdfs:comment "Verknüpft den Downloadlink zu dem aktuellen Holdings-File einer Bibliothek."@de , "Links the Download link to a library's current holdings file"@en ;
rdfs:domain foaf:Organization .

amsl:BibliographicMimeType a owl:Class ;
rdfs:subClassOf dct:MediaTypeOrExtent ;
owl:oneOf (<urn:mimetype:application/marc> <urn:mimetype:application/marcxml+xml>) .

amsl:DownloadHTTP a gr:DeliveryMethod ;
rdfs:label "HTTP Download"@de, "HTTP download"@en .

amsl:DownloadFTP a gr:DeliveryMethod ;
rdfs:label "FTP Download"@de, "FTP download"@en .

amsl:DownloadOAI a gr:DeliveryMethod ;
rdfs:label "OAI Download"@de, "OAI download"@en .

amsl:DownloadAPI a gr:DeliveryMethod ;
rdfs:label "API Download"@de, "API download"@en .



amsl:coveredMediumID a owl:ObjectProperty ;
rdfs:label "abgedeckte Medien (ID)"@de, "covered media"@en ;
rdfs:comment "Verknüpft von den Metadaten der Kollektion abgedeckte Medien über deren IDs (ISSN, ISBN etc.)"@de, 
"Links media covered by the collection's metadata via their IDs (ISSN, ISBN etc.)"@en ;
rdfs:domain amsl:MetadataCollection .



