@prefix bibrm: <http://vocab.ub.uni-leipzig.de/bibrm/> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sysont: <http://ns.ontowiki.net/SysOnt/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


bibrm: a owl:Ontology ;
    rdfs:label "Vocabulary for Library Electronic Resource Management" ;
    cc:attributionName "AKSW & Universitätsbibliothek Leipzig" ;
    vann:changes <https://github.com/ERM-HD/vocab.bibrm/commits/master/bibrm.ttl> ;
    vann:preferredNamespacePrefix "bibrm" ;
    vann:preferredNamespaceUri "http://vocab.ub.uni-leipzig.de/bibrm/" ;
    doap:homepage <http://aksw.org/Projects/ERM> ;
    doap:maintainer <http://aksw.org/NormanRadtke> ;
    foaf:maker <http://aksw.org/AndreasNareike>,
        <http://aksw.org/NatanaelArndt>,
        <http://aksw.org/NormanRadtke>,
        <http://aksw.org/Partner/BjoernMuschall>,
        <http://aksw.org/Partner/LydiaUnterdoerfel>,
        <http://aksw.org/ThomasRiechert> .


######################## Classes #####################################


bibrm:AnnualContractData a rdfs:Class ;
    rdfs:label "Vertragsjahresdaten (konsortial)"@de,
        "annual contract data (consortial)"@en ;
    rdfs:comment "Ein Geschäftsvertrag zwischen der Bibliothek und weiteren Partnern. Dieser Vertragsteil beinhaltet die sich jährlich ändernden Eigenschaften eines Vertrags."@de,
        "A class modeling a business contract between the library and one or more parties. This part of the contract contains the annual (dynamic) properties of a contract"@en .

bibrm:BasicContractData a rdfs:Class ;
    rdfs:label "Vertragsbasisdaten (konsortial)"@de,
        "basic contract data (consortial)"@en ;
    rdfs:comment "Ein Geschäftsvertrag zwischen der Bibliothek und weiteren Partnern. Dieser Vertragsteil beinhaltet die Basisinformationen (statische Daten) eines Vertrages."@de,
        "A class modeling a business contract between the library and one or more parties. This one contains the basic (static) properties of a contract."@en .

bibrm:Budget a rdfs:Class ;
    rdfs:label "Budget"@de,
        "budget"@en ;
    rdfs:comment "Diese Klasse umfasst Budgets für Medien"@de .

bibrm:ContractItem a rdfs:Class ;
    rdfs:label "Vertragsposition"@de,
        "contract item"@en .

bibrm:LicensePackage a rdfs:Class ;
    rdfs:label "Paket"@de,
        "License Package"@en ;
    rdfs:comment "Ein Paket beinhaltet elektronische Bücher oder Zeitschriften, welche eine Bibliothek ihren Nutzern anbietet"@de,
        "A package containing items (e.g. e-journals, e-books) a library offers it's customers"@en .

bibrm:Platform a rdfs:Class ;
    rdfs:label "Plattform"@de,
        "Platform"@en ;
    rdfs:comment "Ein Unternehmen/Service/Portal, welches/r den Inhalt bereitstellt und zugänglich macht (Hosting der ER)"@de,
        "The service that brings the content to your server or puts it on the Web and provides electronic access (hosting of the ER)"@en .

bibrm:Template a rdfs:Class ;
    rdfs:label "Vorlage"@de,
        "Template"@en .


######################## Properties #####################################


bibrm:eissn a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "hat die E-ISSN"@de,
        "has E-ISSN"@en ;
    rdfs:domain bibrm:ContractItem .


bibrm:pissn a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "hat die P-ISSN"@de,
        "has P-ISSN"@en ;
    rdfs:domain bibrm:ContractItem .

bibrm:annualPrice a owl:DatatypeProperty ;
    rdfs:label "Jahrespreis in €"@de,
        "annual price"@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:archivingNote a owl:DataTypeProperty ;
    rdfs:label "Verfügbarkeit nach Vertragsende"@de,
        "availability after termination"@en ;
    rdfs:comment "Informationen zur Verfügbarkeite der ER nach Vertragsende (Archivrecht, Format...)"@de,
        "Additional information related to archiving rights, product, and format"@en .

bibrm:authorizedUser a owl:ObjectProperty ;
    rdfs:label "Autorisierte Nutzer"@de,
        "Authorized User Definition"@en ;
    rdfs:comment "Eine oder mehrere Nutzergruppen, die für das lizensierte Produkt autorisiert sind."@de,
        "Groups of users that are authorized for the licensed product"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:UserGroup .

bibrm:bindsClass a owl:ObjectProperty ;
    rdfs:label "gehört zur Klasse"@de,
        "binds Class"@en .

bibrm:concUser a owl:DatatypeProperty ;
    rdfs:label "Parallelzugriffe"@de,
        "concurrent users"@en ;
    rdfs:comment "Die Anzahl der in den Lizenzbedingungen angegeben Parallelzugriffe"@de,
        "The licensed number of concurrent users for a resource"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:contactNote a owl:DatatypeProperty ;
    rdfs:label "Notiz zum Kontakt"@de,
        "contact note"@en ;
    rdfs:domain foaf:Person .

bibrm:linkToContract a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Link zum Vertrag"@de,
        "url to the contract"@en ;
    rdfs:comment "A url that links to a digital version of the original contract document the licensee recieved from the licensor.",
        "Ein Link/URL, der auf die digitale Version das originalen Vertragsdokuments verweist, welches der Lizenznehmer von Lizenzgeber enthält."@de ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:costCenter a rdf:Property,
        owl:DataTypeProperty ;
    rdfs:label "Kostenstelle"@de,
        "cost center"@en ;
    rdfs:comment "Eine Kostenstelle ist der Ort der Kostenentstehung und der Leistungserbringung. Sie ist ein Kostenrechnungsobjekt mit der Aufgabe, die in einem Unternehmensteil, meist einer Abteilung, angefallenen Kosten zu sammeln."@de,
        "A cost center is part of an organization that does not produce direct profit and adds to the cost of running a company."@en ;
    rdfs:domain <http://purl.org/vocab/aiiso/schema#Faculty> .

bibrm:coursePackE a owl:ObjectProperty ;
    rdfs:label "Reader elektronisch"@de,
        "Course Pack Electronic"@en ;
    rdfs:comment "Das Recht eines Fakultätsmitgliedes lizensiertes Material in Zusammnenstellungen in elektronischer Form für Studentengruppen zu Anleitungszwecken zu verwenden."@de,
        "The right to use licensed materials in collections or compilations of materials assembled in an electronic format by faculty members for use by students in rdfs:type class for purposes of instruction"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:coursePackP a owl:ObjectProperty ;
    rdfs:label "Reader print"@de,
        "Course Pack Print"@en ;
    rdfs:comment "Das Recht eines Fakultätsmitgliedes lizensiertes Material in Zusammnenstellungen in gedruckter Form für Studentengruppen zu Anleitungszwecken zu verwenden."@de,
        "The right to use licensed materials in collections or compilations of materials assembled in rdfs:type print format by faculty members for use by students in rdfs:type class for purposes of instruction."@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:courseReserveE a owl:ObjectProperty ;
    rdfs:label "Semesterapparat elektronisch"@de,
        "Course Reserve Electronic"@en ;
    rdfs:comment "Das Recht digitale Kopien des lizensierten Materials zu erstellen und sie in einem sicheren Netzwerk bereitzustellen."@de,
        "The right to make electronic copies of the licensed materials and store them on rdfs:type secure network"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:courseReserveP a owl:ObjectProperty ;
    rdfs:label "Semesterapparat print"@de,
        "Course Reserve Print"@en ;
    rdfs:comment "Das Recht gedruckte Kopien des lizensierten Materials zu erstellen und sie in einem kontrollierten Bereich bereitzustellen.",
        "Das Recht digitale Kopien des lizensierten Materials zu erstellen und sie in einem sicheren Netzwerk bereitzustellen."@de,
        " The right to make print copies of the licensed materials and place them in rdfs:type controlled circulation area of the library for reserve reading in conjunction with specific courses of instruction"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:dataAvailability a owl:DatatypeProperty ;
    rdfs:label "(Meta-)Datenverfügbarkeit"@de,
        "(meta) data availability"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:description a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Beschreibung"@de,
        "description"@en ;
    rdfs:comment "Die Beschreibung des Inhalts der ER. Es können Links zu weiteren relevanten Dokumenten (z. B. Inhaltsverzeichnis) enthalten sein. Quelle der Definition: Dublin Core Metadata Initiative"@de,
        "An account of the content of the resource. It may include pointers to other relevant objects (e.g., tables of contents). Source of definition: Dublin Core Metadata Initiative"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:linkToDescription a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "URL der Anbieterbeschreibung"@de,
        "URL to the description "@en ;
    rdfs:comment "Link zur Beschreibung der ER des Anbieters"@de,
        "The URL to the description of the creator/privider of an Electronic Product"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:bibrm:digitallyCopy a owl:ObjectProperty ;
    rdfs:label "Kopie digital"@de,
        "Digital Copy "@en ;
    rdfs:comment "Das Recht des Lizenznehmers und autorisierter Nutzer, eine digitale Kopie zu erstellen."@de,
        "Das Recht eines autorisierten Nutzers von außerhalb Zugang zu elektronischen Produkten zu erlangen"@de,
        "The right of an authorized user to gain access to an Electronic Product from an offsite location"@en,
        "The right of the licensee and authorized users to download and digitally copy rdfs:type reasonable portion of the licensed materials."@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:erAltTitle a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Alternativer Titel"@de,
        "alternate titel"@en ;
    rdfs:comment "Eine Variation des Haupttitels der Ressource, z.B. Akronyme, Paralleltitel usw."@de,
        "A variation in the title proper of the resource, including acronyms, parallel titles, titles in other languages, etc."@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:expectedPrice a owl:DatatypeProperty ;
    rdfs:label "Planpreis in €"@de,
        "expected price"@en ;
    rdfs:comment "Der Preis, welcher im vorhergehenden Jahr für die Ressource eingeplant wurde"@de,
        "The price for the ER you have expected last year"@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:externalContact a owl:ObjectProperty ;
    rdfs:label "Externer Ansprechpartner"@de,
        "external contact"@en ;
    rdfs:comment "Der verantwortliche Ansprechpartner auf Seiten von Verlagen, Konsortien..."@de,
        "A contact person from publisher, licensing agent, consortium... for the contract"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range foaf:Person .

bibrm:feedbackDate a owl:DatatypeProperty ;
    rdfs:label "Rückmeldetermin"@de,
        "Feedback date"@en ;
    rdfs:comment "Der letztmögliche Tag, bis zu welchem eine Rückmeldung an Zahlungsempfänger, Lizenzgeber etc. erfolgen muss ist."@de,
        "The last possible date till which a feedback to licensing agent, licensor etc. must be given."@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range xsd:date .

bibrm:field a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Fachgebiet"@de,
        "field"@en ;
    rdfs:comment "Das Prädikat ermöglicht die Angabe von Fachgebieten"@de,
        "The field predicate can be used to classify bibliographic items"@en ;
    rdfs:domain <http://purl.org/vocab/aiiso/schema#Faculty> .

bibrm:acqMethod a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Erwerbungsart"@de,
        "Acquisition Method"@en ;
    rdfs:range bibrm:AcqNote .

bibrm:annualContractData a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "zugehörige Vertragsjahresdaten"@de,
        "assigned annual contact data"@en ;
    rdfs:comment "The annual contact data assigned to the contract",
        "Die Vertragsjahresdaten, die zum Vertrag gehören"@de ;
    rdfs:domain bibrm:LicensePackage ;
    rdfs:range bibrm:AnnualContractData .

bibrm:authMethod a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Authentifizierungsmethode"@de,
        "Authentication Method"@en ;
    rdfs:range bibrm:AuthMethod .

bibrm:basicContractData a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Zugehörige Vertragsbasisdaten"@de,
        "associated basic contract data"@en ;
    rdfs:comment "Link zu Vertragsbasisdaten, die durch jährliche Vertragsdaten ergänzt werden"@de,
        "link to basic contract data which are complemented by annual contract data"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range bibrm:BasicContractData .

bibrm:budget a owl:ObjectProperty ;
    rdfs:label "hat Budget"@de,
        "has budget"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range bibrm:Budget .

bibrm:consortium a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Beteiligtes Konsortium"@de,
        "consortium"@en ;
    rdfs:comment "Ein Zusammenschluss von Bibliotheken. Immer einzutragen, auch wenn mit weiteren Feldern (Zahlungsempfänger, Plattform, Hersteller [Creator], Lizenzgeber) identisch." ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range foaf:Organization .

bibrm:dbType a owl:ObjectProperty ;
    rdfs:label "hat Datenbanktyp"@de,
        "has database type"@en ;
    rdfs:range bibrm:DBType .

bibrm:erStatus a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Status der ER"@de,
        "Status of the ER"@en ;
    rdfs:range bibrm:ERStatus .

bibrm:erType a owl:ObjectProperty ;
    rdfs:label "Produktart"@de,
        "ER type"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:ERType .

bibrm:expectedInvoiceMonth a owl:ObjectProperty ;
    rdfs:label "Erwarteter Rechnungsmonat"@de,
        "Expected Invoice Month"@en ;
    rdfs:range bibrm:Month .

bibrm:illNote a owl:DatatypeProperty ;
    rdfs:label "Fernleihnotiz"@de,
        "Interlibrary Loan Note"@en ;
    rdfs:comment "Dieses Feld dient der Erfassung weiterer Informationen zur Fernleihe"@de,
        "Used to note further information about Interlibrary Loan"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:illset a owl:ObjectProperty ;
    rdfs:label "Interlibrary Loan Secure Electronic Transmission"@de,
        "Interlibrary Loan Secure Electronic Transmission"@en ;
    rdfs:range bibrm:LicenseTerm .

bibrm:invoiceRecieved a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Rechnung vorhanden?"@de,
        "Invoice recieved?"@en ;
    rdfs:range bibrm:Boolean .

bibrm:contractItem a owl:ObjectProperty ;
    rdfs:label "hat Vertragsposition"@de,
        "has item"@en ;
    rdfs:comment "Links to items that are part of a license package or a licnense contract"@en ;
    rdfs:domain bibrm:AnnualContractData,
        bibrm:LicensePackage ;
    rdfs:range bibrm:ContractItem .

bibrm:noticePeriod a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Kündigungsfrist"@de,
        "Notice Period"@en ;
    rdfs:comment "Die Kündigungsfrist ist zu verwenden, wenn die vorliegenden Angaben kein Datum darstellen oder nicht sinnvoll in ein solches umgerechnet werden können"@de,
        "The notice period must be recorded if no exact date can be identified"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:organizationType a owl:ObjectProperty ;
    rdfs:label "Kategorie"@de,
        "organization type"@en ;
    rdfs:domain foaf:Organization ;
    rdfs:range bibrm:OrganizationType .

bibrm:licensePackage a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "hat Paket"@de,
        "has package"@en ;
    rdfs:comment "A package, which belongs to an instance of annual contract data",
        "Ein Paket, welches zu einer Instanz von Vertragsjahresdaten (VJD) gehört" ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range bibrm:LicensePackage .

bibrm:platform a rdf:property,
        owl:ObjectProperty ;
    rdfs:label "via Plattform"@de,
        "via platform"@en ;
    rdfs:comment "Verknüpft die Vertragsbasisdaten mit der Plattform, über die die Inhalte abrufbar sind."@de,
        "Connects basic contract data with the associated platform"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:Platform .

bibrm:primaryAccessURI a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "URL des Mediums"@de,
        "URL of the ER"@en ;
    rdfs:comment "The method of access, the location, and the file name of an Electronic Product"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:shibbolethImplStatus a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Status der Shibboleth Implementation"@de,
        "Status of shibboleth implementation"@en ;
    rdfs:domain bibrm:Platform ;
    rdfs:range bibrm:ShibbolethImplStatus .

bibrm:illElectronic a owl:ObjectProperty ;
    rdfs:label "Fernleihe elektronisch"@de,
        "Interlibrary  Loan Electronic"@en ;
    rdfs:comment "Das Recht lizensiertes Material in Form einer digitalen Kopie für die Fernleihe zur Verfügung zu stellen."@de,
        "The right to provide the licensed materials via interlibrary loan by way of electronic copies"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:illPrintOrFax a owl:ObjectProperty ;
    rdfs:label "Fernleihe print / Fax"@de,
        "Interlibrary Loan Print or Fax"@en ;
    rdfs:comment "Das Recht lizensiertes Material in Form einer Print- oder Faxkopie für die Fernleihe zur Verfügung zu stellen."@de,
        "The right to provide the licensed materials via interlibrary loan by way of print copies or facsimile transmission"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:initialAcquisitionDate a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Ersterwerbungsdatum"@de,
        "initial acquisition date"@en ;
    rdfs:comment "Das Datum, an welchem das Produkt zum ersten Mal erworben wurde",
        "The date on which the license was purchased by the licensee for the first time"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range xsd:date .

bibrm:internalContact a owl:ObjectProperty ;
    rdfs:label "Hausinterner Ansprechpartner"@de,
        "inhouse contact"@en ;
    rdfs:comment "Der verantwortliche Ansprechpartner auf Seiten des Lizenznehmers"@de,
        "An inhouse contact person for the contract"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range foaf:Person .

bibrm:invoiceDate a owl:DatatypeProperty ;
    rdfs:label "Rechnungsbearbeitungs-Datum"@de,
        "Invoive processing date"@en ;
    rdfs:comment "Der Tag, an dem die Rechnung bearbeitet wurde"@de,
        "The day the invoice was processed"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range xsd:date .

bibrm:invoicePrice a owl:DatatypeProperty ;
    rdfs:label "Rechnungspreis in €"@de,
        "invoice price"@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:isMemberOf a owl:ObjectProperty ;
    rdfs:label "ist Mitglied von"@de,
        "is member of"@en ;
    rdfs:domain foaf:Person ;
    rdfs:range foaf:Organization .

bibrm:isRelatedTo a owl:ObjectProperty ;
    rdfs:range <http://purl.org/vocab/aiiso/schema#Faculty> .

bibrm:licenseEndDate a owl:DatatypeProperty ;
    rdfs:label "Vertragsende"@de,
        "license end date"@en ;
    rdfs:comment "Das Datum, an welchem die Gültigkeit eines Lizenzvertrags endet"@en,
        "Das Datum, an welchem ein Lizenzvertrag endet"@en,
        "The date on which a license agreement loses it's effect"@en,
        "The date on which a license agreement terminates"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range xsd:date .

bibrm:licenseStartDate a owl:DatatypeProperty ;
    rdfs:label "Vertragsbeginn"@de,
        "license start date"@en ;
    rdfs:comment "Das Datum, an welchem ein Lizenzvertrag in Kraft tritt"@en,
        "The date on which a license agreement comes into effect"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range xsd:date .

bibrm:licenseType a owl:ObjectProperty,
        owl:ObjectPropery ;
    rdfs:label "Lizenzart"@de,
        "license type"@en ;
    rdfs:comment "Art der Lizenz, welche Zugang zur ER erlaubt"@de,
        "The kind of license that governs access to the product"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range bibrm:LicenseType .

bibrm:licensee a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Lizenznehmer"@de,
        "licensee"@en ;
    rdfs:comment "Die Vertragspartei, die eine Lizenz erhält"@de,
        "The party of a contract receiving the lawfull object/license"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range foaf:Organization .

bibrm:licensingNote a owl:DatatypeProperty ;
    rdfs:label "Bemerkungen zum Vertrag"@de,
        "licensing note"@en ;
    rdfs:comment "Zusätzliche Informationen, welche den Vertrag, die Lizenzverhandlungen, das Produkt etc. betreffen."@de,
        "Additional information regarding the license, the negotiation of the license, the product, etc."@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:licensingAgent a owl:ObjectProperty ;
    rdfs:label "Zahlungsempfänger"@de,
        "licensing agent"@en ;
    rdfs:comment "An organization (such as a subscription agent) or an individual that facilitates a licensing transaction on behalf of one or more parties",
        """Ein Unternehmen, das die Bezahlung einer ER abwickelt. Immer einzutragen, auch wenn mit weiteren Feldern (Konsortium, Plattform, Hersteller [Creator], Lizenzgeber) identisch."""@de,
        "An organization (such as a subscription agent) or an individual that facilitates a licensing transaction on behalf of one or more parties"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range foaf:Organization .

bibrm:licensor a owl:ObjectProperty ;
    rdfs:label "Lizenzgeber"@de,
        "licensor"@en ;
    rdfs:comment """Die Vertragspartei, die eine Lizenz zur Verfügung stellt. Für uns wird hier i.d.R. der Verlag eingetragen. Immer einzutragen, auch wenn mit weiteren Feldern (Konsortium, Zahlungsempfänger, Plattform, Hersteller [Creator]) identisch.\r
"""@de,
        "The party of a contract offering the lawfull object/license"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range foaf:Organization .

bibrm:linkToBalance a owl:DatatypeProperty ;
    rdfs:label "Link zum Über-/Fehlbetrag"@de,
        "link to balance"@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:linkToInvoice a owl:DatatypeProperty ;
    rdfs:label "Link zur Rechnung"@de,
        "link to invoice"@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:localSystemNumber a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Nummer aus Lokalsystem"@de,
        "local system number"@en ;
    rdfs:comment "Die Nummer, die das lokale Bibliotheksmanagementsystem (z.B. Libero) für die Ressource vergibt"@de,
        "The record number that is assigned by a local integrated library system (ILS) to the bibliographic record for the resource"@en ;
    rdfs:domain bibrm:AnnualContractData,
        bibrm:ContractItem,
        bibrm:LicensePackage .

bibrm:optionalProperty a owl:ObjectProperty ;
