@prefix bibrm: <http://vocab.ub.uni-leipzig.de/bibrm/> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sysont: <http://ns.ontowiki.net/SysOnt/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix aiiso: <http://purl.org/vocab/aiiso/schema#> .


bibrm: a owl:Ontology ;
    rdfs:label "Vocabulary for Library Electronic Resource Management" ;
    cc:attributionName "AKSW & Universitätsbibliothek Leipzig" ;
    vann:changes <https://github.com/ERM-HD/vocab.bibrm/commits/master/bibrm.ttl> ;
    vann:preferredNamespacePrefix "bibrm" ;
    vann:preferredNamespaceUri "http://vocab.ub.uni-leipzig.de/bibrm/" ;
    doap:homepage <http://aksw.org/Projects/ERM> ;
    doap:maintainer <http://aksw.org/NormanRadtke> ;
    foaf:maker <http://aksw.org/AndreasNareike>,
        <http://aksw.org/NatanaelArndt>,
        <http://aksw.org/NormanRadtke>,
        <http://aksw.org/Partner/BjoernMuschall>,
        <http://aksw.org/Partner/LydiaUnterdoerfel>,
        <http://aksw.org/ThomasRiechert> .


######################## Classes #####################################


bibrm:AnnualContractData a rdfs:Class ;
    rdfs:label "Vertragsjahresdaten"@de,
        "Annual Contract Data"@en ;
    rdfs:comment "Ein Geschäftsvertrag zwischen der Bibliothek und weiteren Perteien. Dieser Vertragsteil beinhaltet die sich jährlich ändernden Eigenschaften eines Vertrags."@de,
        "A class modeling a business contract between the library and one or more parties. This part of the contract contains the annual (dynamic) properties of a contract."@en .

bibrm:BasicContractData a rdfs:Class ;
    rdfs:label "Vertragsbasisdaten"@de,
        "Basic Contract Data"@en ;
    rdfs:comment "Ein Geschäftsvertrag zwischen der Bibliothek und weiteren Parteien. Dieser Vertragsteil beinhaltet die Basisinformationen (statische Daten) eines Vertrages."@de,
        "A class modeling a business contract between the library and one or more parties. This one contains the basic (static) properties of a contract."@en .

bibrm:Budget a rdfs:Class ;
    rdfs:label "Budget"@de,
        "Budget"@en ;
    rdfs:comment "Budgets für die Medienerwerbung."@de,
    	 "Budgets for media acquisition."@en .

bibrm:ContractItem a rdfs:Class ;
    rdfs:label "Vertragsposition"@de,
        "Contract Item"@en ;
    rdfs:comment "Die einzelnen elektronischen Ressourcen, die von einem Vetrag oder einem Lizenzpaket umfasst werden."@de ,
    	"Individual electronic resources that are contained in a license contract or package."@en .

bibrm:LicensePackage a rdfs:Class ;
    rdfs:label "Paket"@de,
        "License Package"@en ;
    rdfs:comment "Ein Lizenzvertrag kann aus mehreren Lizenzpaketen zusammengestellt sein."@de,
        "A license contract can be made up by several license packages."@en .

bibrm:Platform a rdfs:Class ;
    rdfs:label "Plattform"@de,
        "Platform"@en ;
    rdfs:comment "Das Portal bzw. der Service, der die Inhalte bereitstellt und zugänglich macht."@de,
        "The portal resp. service providing the electronic content and the access to it."@en .

   


######################## Properties #####################################
bibrm:contractItemOf a owl:ObjectProperty ;
    rdfs:label "ist Vertragsposition von"@de,
        "is contract item of"@en ;
    rdfs:comment "Verknüpft eine bestimmte Vertragsposition mit den Vertragsjahresdaten bzw. dem  Lizenzpaket, in dem sie enthalten ist."@de,
    	"Links a certain contract item with the annual contract data resp. the license package it is contained in."@en ;
    rdfs:range [ a owl:Class ;
    	owl:unionOf (bibrm:AnnualContractData  bibrm:LicensePackage)] ;
    rdfs:domain bibrm:ContractItem  .

bibrm:contractContainingPackage a owl:ObjectProperty ;
    rdfs:label "das Paket umfassender Vertrag"@de,
        "contract containing the package"@en ;
    rdfs:comment "Verknüpft die Vertragsjahresdaten, zu denen ein bestimmtes Lizenzpaket gehört."@de ,
    	"Links the the annual contact data a certain license package is associated with."@en ;        
    rdfs:domain bibrm:LicensePackage ;
    rdfs:range bibrm:AnnualContractData .

bibrm:eissn a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "E-ISSN"@de,
        "E-ISSN"@en ;
    rdfs:comment "Verknüpft die E-ISSN eines Mediums."@de,
    	"Links the E-ISSN of a medium."@en .

bibrm:eisbn a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "E-ISBN"@de,
        "E-ISBN"@en ;
    rdfs:comment "Verknüpft die E-ISBN eines Mediums."@de,
    	"Links the E-ISBN of a medium."@en .

bibrm:pissn a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "P-ISSN"@de,
        "P-ISSN"@en ;
    rdfs:comment "Verknüpft die P-ISSN eines Mediums."@de,
    	"Links the P-ISSN of a medium."@en .

bibrm:pisbn a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "P-ISBN"@de,
        "P-ISBN"@en ;
    rdfs:comment "Verknüpft die P-ISBN eines Mediums."@de,
    	"Links the P-ISBN of a medium."@en .
    
bibrm:proprietaryID a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Proprietäre ID"@de,
        "Proprietary ID"@en ;
    rdfs:comment "Verknüpft einen proprietären Identifier eines Mediums."@de,
    	"Links a proprietary identifier of a medium."@en .
    	
bibrm:doi a rdf:Property,
        owl:ObjectProperty ; 
    rdfs:label "DOI"@de,
        "DOI"@en ;
    rdfs:comment "Verknüpft den DOI eines Mediums."@de,
    	"Links the DOI of a medium."@en .

bibrm:annualPrice a owl:DatatypeProperty ;
    rdfs:label "Jahrespreis (EUR)"@de,
        "annual price (EUR)"@en ;
    rdfs:comment "Verknüpft den jährlichen Preis eines Lizenzvertrages in Euro."@de,
    	"Links the annual price of a license contract in euro."@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:archivingNote a owl:DataTypeProperty ;
    rdfs:label "Verfügbarkeit nach Vertragsende"@de,
        "availability after termination"@en ;
    rdfs:comment "Verknüpft Informationen zur Verfügbarkeite der ER nach Vertragsende (Archivrecht, Format...)."@de,
         "Links additional information related to archiving rights, product, and format."@en ;
    rdfs:domain bibrm:BasicContractData .     

bibrm:authorizedUser a owl:ObjectProperty ;
    rdfs:label "Autorisierte Nutzer"@de,
        "authorized users"@en ;
    rdfs:comment "Verknüpft eine oder mehrere Nutzergruppen, die für zur Nutzung des lizenzierten Produktes autorisiert sind."@de,
        "Links one or several usersgroups that are authorized to use the licensed product."@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:UserGroup .

bibrm:concUser a owl:DatatypeProperty ;
    rdfs:label "Parallelzugriffe"@de,
        "concurrent users"@en ;
    rdfs:comment "Verknüpft die Anzahl der in den Lizenzbedingungen angegeben Parallelzugriffe"@de,
        "Links the licensed number of concurrent users for a resource"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:contactNote a owl:DatatypeProperty ;
    rdfs:label "Notiz zum Kontakt"@de,
        "contact note"@en ;
    rdfs:comment "Verknüft weitere notierte Details zu einem Kontakt"@de,
    	"Links further listed details on a contact"@en ;
    rdfs:domain foaf:Person .

bibrm:linkToContract a owl:DatatypeProperty ;
    rdfs:label "Link zum Vertrag"@de,
        "url to the contract"@en ;
    rdfs:comment "Verknüpft einen Link/eine URL, die auf die digitale Version das originalen Vertragsdokuments verweist, welches der Lizenznehmer vom Lizenzgeber enthält."@de,
    	"Links a url linking to a digital version of the original contract document the licensee received from the licensor."@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:costCenter a rdf:Property,
        owl:DataTypeProperty ;
    rdfs:label "Kostenstelle"@de,
        "cost center"@en ;
    rdfs:comment "Verknüpft eine Kostenstelle. Diese ist der Ort der Kostenentstehung und der Leistungserbringung. Sie ist ein Kostenrechnungsobjekt mit der Aufgabe, die in einem Unternehmensteil, meist einer Abteilung, angefallenen Kosten zu sammeln."@de,
        "Links a cost center. It is part of an organization that does not produce direct profit and adds to the cost of running a company."@en ;
    rdfs:domain aiiso:Faculty .

bibrm:coursePackE a owl:ObjectProperty ;
    rdfs:label "Reader elektronisch"@de,
        "Course Pack Electronic"@en ;
    rdfs:comment "Verknüpft Infomationen über das Recht eines Fakultätsmitgliedes lizenziertes Material in Zusammnenstellungen in elektronischer Form für Studentengruppen zu Anleitungszwecken zu verwenden."@de,
        "Links information on the right to use licensed materials in collections or compilations of materials assembled in an electronic format by faculty members for use by students in a class for purposes of instruction"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:coursePackP a owl:ObjectProperty ;
    rdfs:label "Reader print"@de,
        "Course Pack Print"@en ;
    rdfs:comment "Verknüpft Infomationen über das Recht eines Fakultätsmitgliedes lizenziertes Material in Zusammnenstellungen in gedruckter Form für Studentengruppen zu Anleitungszwecken zu verwenden."@de,
        "Links information on the right to use licensed materials in collections or compilations of materials assembled in a print format by faculty members for use by students in a class for purposes of instruction."@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:courseReserveE a owl:ObjectProperty ;
    rdfs:label "Semesterapparat elektronisch"@de,
        "Course Reserve Electronic"@en ;
    rdfs:comment "Verknüpft Infomationen über das Recht digitale Kopien des lizenzierten Materials zu erstellen und sie in einem sicheren Netzwerk bereitzustellen."@de,
        "Links information on the right to make electronic copies of the licensed materials and store them on a secure network"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:courseReserveP a owl:ObjectProperty ;
    rdfs:label "Semesterapparat print"@de,
        "Course Reserve Print"@en ;
    rdfs:comment "Verknüpft Infomationen über das Recht gedruckte Kopien des lizenzierten Materials zu erstellen und sie in einem kontrollierten Bereich bereitzustellen."@de,
        "Links information on the right to make print copies of the licensed materials and place them in a controlled circulation area of the library for reserve reading in conjunction with specific courses of instruction"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:dataAvailability a owl:DatatypeProperty ;
    rdfs:label "(Meta-)Datenverfügbarkeit"@de,
        "(meta) data availability"@en ;
    rdfs:comment "Verknüpft Informationen über die Verfügbarkeit von Metadaten zu den lizenzierten Medien"@de ,
    	"Links information on the availability of metadata for the licensed media."@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:description a owl:DatatypeProperty ;
    rdfs:label "Beschreibung"@de,
        "description"@en ;
    rdfs:comment "Verknüpft die Beschreibung des Inhalts der ER. Es können Links zu weiteren relevanten Dokumenten (z. B. Inhaltsverzeichnis) enthalten sein."@de,
        "Links an account of the content of the resource. It may include pointers to other relevant objects (e.g., tables of contents)"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:linkToDescription a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "URL der Anbieterbeschreibung"@de,
        "URL to the description "@en ;
    rdfs:comment "Verknüpft einen Link zur Beschreibung der ER des Anbieters"@de,
        "Links the URL to the description of the creator/privider of an Electronic Product"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:digitallyCopy a owl:ObjectProperty ;
    rdfs:label "Kopie digital"@de,
        "Digital Copy "@en ;
    rdfs:comment "Verknüpft Infomationen über das Recht des Lizenznehmers und autorisierter Nutzer, eine digitale Kopie zu erstellen."@de,
        "Links information on the right of the licensee and authorized users to download and digitally copy a reasonable portion of the licensed materials."@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:erAltTitle a owl:DatatypeProperty ;
    rdfs:label "Alternativer Titel"@de,
        "alternative title"@en ;
    rdfs:comment "Verknüpft eine Variation des Haupttitels der Ressource, z.B. Akronyme, Paralleltitel usw."@de,
        "Links a variation of the title of the resource, including acronyms, parallel titles, titles in other languages, etc."@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:expectedPrice a owl:DatatypeProperty ;
    rdfs:label "Planpreis laufendes Jahr (EUR)"@de,
        "expected price current year (EUR)"@en ;
    rdfs:comment "Verknüpft den Preis, welcher für das laufende Jahr für den Vertrag geschätzt und eingeplant wurde"@de,
        "Links the price that was expected and planned for the license for the current year"@en ;
    rdfs:domain bibrm:AnnualContractData .
    
bibrm:expectedPriceFollowingYear a owl:DatatypeProperty ;
    rdfs:label "Planpreis Folgejahr (EUR)"@de,
        "expected price following year (EUR)"@en ;
    rdfs:comment "Verknüpft den Preis, welcher für das nachfolgende Jahr für den Vertrag geschätzt und eingeplant wurde"@de,
        "Links the price that was expected and planned for the license for the following year"@en ;
    rdfs:domain bibrm:AnnualContractData .    

bibrm:externalContact a owl:ObjectProperty ;
    rdfs:label "Externer Ansprechpartner"@de,
        "external contact"@en ;
    rdfs:comment "Verknüpft den verantwortlichen Ansprechpartner auf Seiten von Verlagen, Konsortien..."@de,
        "Links the responsible contact person from publisher, licensing agent, consortium..."@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range foaf:Person .

bibrm:feedbackDate a owl:DatatypeProperty ;
    rdfs:label "Rückmeldetermin"@de,
        "Feedback date"@en ;
    rdfs:comment "Verknüpft den letztmöglichen Tag, an welchem eine Rückmeldung an Zahlungsempfänger, Lizenzgeber etc. erfolgen muss"@de,
        "Links the last possible date to give feedback to licensing agent, licensor etc."@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range xsd:date .

bibrm:field a owl:DatatypeProperty ;
    rdfs:label "Fachgebiet"@de,
        "field"@en ;
    rdfs:comment "Verknüpft Fakultäten mit ihren Fachgebieten"@de,
        "Links faculties with their field of activity"@en ;
    rdfs:domain aiiso:Faculty .

bibrm:acqMethod a owl:ObjectProperty ;
    rdfs:label "Erwerbungsart"@de,
        "Acquisition Method"@en ;
    rdfs:comment "Verknüpft die Art, auf die die Lizenzen des Vertrages erworben werden"@de,
    	"Links the method of acquisition used to purchase the licenses"@en ;    
    rdfs:domain bibrm:BasicContractData ;    
    rdfs:range bibrm:AcqNote .

bibrm:authMethod a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Authentifizierungsmethode"@de,
        "Authentication Method"@en ;
    rdfs:comment "Verknüpft die nötige Authentifizierungsmethode, um Zugang zu den lizenzierten Medien zu erhalten"@de,
    	"Links the method of authentification requiered to gain access to the licensed media"@en ;        
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:AuthMethod .

bibrm:basicContractData a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Zugehörige Vertragsbasisdaten"@de,
        "associated basic contract data"@en ;
    rdfs:comment "Vernüpft die Vertragsbasisdaten, zu denen bestimmte Vertragsbasisdaten gehören"@de,
        "Links the basict contract data, certain annual contract data are associated with"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range bibrm:BasicContractData .

bibrm:budget a owl:ObjectProperty ;
    rdfs:label "hat Budget"@de,
        "has budget"@en ;
    rdf:comment "Verknüpft das Budget, aus dem die Kosten für einen Lizenzvertrag gezahlt werden."@de,
    	"Links the budget charged with the costs of a license contract"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range bibrm:Budget .

bibrm:consortium a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Beteiligtes Konsortium"@de,
        "consortium"@en ;
    rdfs:comment "Verknüpft ein beteiligtes Bibliothekskonsortium, d. h. einen Zusammenschluss von Bibliotheken"@de,
    	"Links a n involved library consortium, that is an association of libraries"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range foaf:Organization .

bibrm:dbType a owl:ObjectProperty ;
    rdfs:label "hat Datenbanktyp"@de,
        "has database type"@en ;
    rdfs:comment "Verknüpft den Typ einer Datenbank"@de ,
    	"Links the type of a database"@en ;    
    rdfs:domain bibrm:BasicContractData ;    
    rdfs:range bibrm:DBType .

bibrm:erStatus a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Status der ER"@de,
        "Status of the ER"@en ;
    rdfs:comment "Verknüpft den Status der elektronischen Ressourcen bzw. des diese beinhaltenden Vertrages im Erwerbungsprozess"@de,
    	"Links the status of the electronic resources resp. of the containing contract in the process of acquisition."@en ;
    rdfs:domain bibrm:BasicContractData ;    
    rdfs:range bibrm:ERStatus .

bibrm:erType a owl:ObjectProperty ;
    rdfs:label "Produktart"@de,
        "ER type"@en ;
    rdfs:comment "Verknüpft die Produktart der in einem Vertrag lizenzierten elektronischen Ressourcen"@de,
    	"Links the type of product of the electronic resources licensed in a contract"@en ; 
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:ERType .

bibrm:expectedInvoiceMonth a owl:ObjectProperty ;
    rdfs:label "Erwarteter Rechnungsmonat"@de,
        "Expected Invoice Month"@en ;
    rdfs:comment "Verknüpft den Monat, in dem der Eingang der Rechnung zu einem Vertrag erwartet wird."@de, 
    	"Línks the month the invoice receipt for a contract is expected"@en ;   
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:Month .

bibrm:illNote a owl:DatatypeProperty ;
    rdfs:label "Fernleihnotiz"@de,
        "Interlibrary Loan Note"@en ;
    rdfs:comment "Verknüpft weitere Informationen über Fernleihbedingungen"@de,
        "Links further information on terms concerning the Interlibrary Loan"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:illset a owl:ObjectProperty ;
    rdfs:label "Interlibrary Loan Secure Electronic Transmission"@de,
        "Interlibrary Loan Secure Electronic Transmission"@en ;
    rdfs:comment "Verknüpft Informationen über ILLSET"@de,
    	"Links information on ILLSET"@en ;        
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:invoiceReceived a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "Rechnung vorhanden?"@de,
        "Invoice received?"@en ;
    rdfs:comment "Verknüpft die Information, ob die aktuelle Rechnung bereits eingegangen ist"@de ,
    	"Links the information, if the current invoice has alreday been received"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range bibrm:Boolean .

bibrm:noticePeriod a owl:DatatypeProperty ;
    rdfs:label "Kündigungsfrist"@de,
        "Notice Period"@en ;
    rdfs:comment "Verknüpft die einzuhaltende Kündigungsfrist eines Vertrages. Die Kündigungsfrist ist zu verwenden, wenn die vorliegenden Angaben kein Datum darstellen oder nicht sinnvoll in ein solches umgerechnet werden können"@de,
        "Links the the notice period to be adhered. The notice period must be recorded if no exact date can be identified"@en ;
    rdfs:domain bibrm:AnnualContractData .

bibrm:organizationType a owl:ObjectProperty ;
    rdfs:label "Kategorie"@de,
        "organization type"@en ;
    rdfs:comment "Verknüpft die Kategorie einer Organisation"@de,
    	"Links the category resp. type of an organization"@en ;    
    rdfs:domain foaf:Organization ;
    rdfs:range bibrm:OrganizationType .

bibrm:platform a  owl:ObjectProperty ;
    rdfs:label "via Plattform"@de,
        "via platform"@en ;
    rdfs:comment "Verknüpft die Vertragsbasisdaten mit der Plattform, über die die Inhalte abrufbar sind."@de,
        "Links basic contract data with the associated platform"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:Platform .

bibrm:primaryAccessURI a  owl:DatatypeProperty ;
    rdfs:label "URL des Mediums"@de,
        "URL of the ER"@en ;
    rdfs:comment "Verknüpft die URI, unter der die lizenzierten Medien primär zugänglich sind"@de,
    	"Links the URI under which the licensed media is primarily accessible"@en ;
    rdfs:domain bibrm:BasicContractData .

bibrm:shibbolethImplStatus a owl:ObjectProperty ;
    rdfs:label "Status Shibboleth Implementation"@de, "Shibboleth implementation status"@en ;
    rdfs:comment "Verknüpft Informationen über den Status der Shibboleth Implementation"@de,
        "Links information on the status of the shibboleth implementation"@en ;
    rdfs:domain bibrm:Platform ;
    rdfs:range bibrm:ShibbolethImplStatus .

bibrm:illElectronic a owl:ObjectProperty ;
    rdfs:label "Fernleihe elektronisch"@de,
        "Interlibrary  Loan Electronic"@en ;
    rdfs:comment "Verknüpft Informationen über das Recht lizenziertes Material in Form einer digitalen Kopie für die Fernleihe zur Verfügung zu stellen."@de,
        "Links information on the right to provide the licensed materials via interlibrary loan by way of electronic copies"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:illPrintOrFax a owl:ObjectProperty ;
    rdfs:label "Fernleihe print / Fax"@de,
        "Interlibrary Loan Print or Fax"@en ;
    rdfs:comment "Verknüpft Informationen über das Recht lizenziertes Material in Form einer Print- oder Faxkopie für die Fernleihe zur Verfügung zu stellen."@de,
        "Links information on the right to provide the licensed materials via interlibrary loan by way of print copies or facsimile transmission"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range bibrm:LicenseTerm .

bibrm:initialAcquisitionDate a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "Ersterwerbungsdatum"@de,
        "initial acquisition date"@en ;
    rdfs:comment "Verknüpft das Datum, an welchem das Produkt zum ersten Mal erworben wurde"@de,
        "Links the date the license was purchased by the licensee for the first time"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range xsd:date .

bibrm:internalContact a owl:ObjectProperty ;
    rdfs:label "Hausinterner Ansprechpartner"@de,
        "inhouse contact"@en ;
    rdfs:comment "Verknüpft den verantwortlichen Ansprechpartner auf Seiten des Lizenznehmers"@de,
        "Links the responsible inhouse contact person for the contract"@en ;
    rdfs:domain bibrm:BasicContractData ;
    rdfs:range foaf:Person .

bibrm:invoiceDate a owl:DatatypeProperty ;
    rdfs:label "Rechnungsbearbeitungs-Datum"@de,
        "Invoive processing date"@en ;
    rdfs:comment "Verknüpft den Tag, an dem die Rechnung bearbeitet wurde"@de,
        "links the day the invoice was processed"@en ;
    rdfs:domain bibrm:AnnualContractData ;
    rdfs:range xsd:date .
   

bibrm:invoicePriceNetEUR a owl:DatatypeProperty;
	rdfs:label "Rechnungspreis netto (EUR)"@de,
		"invoice price net (EUR)"@en ;
	rdfs:comment "Verknüpft den netto Rechnungspreis in Euro."@de,
		"Links the invoice price net in euro."@en ;
	rdfs:domain bibrm:AnnualContractData .
	

bibrm:invoicePriceGrossEUR  a owl:DatatypeProperty;
